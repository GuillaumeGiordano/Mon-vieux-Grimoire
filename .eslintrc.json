{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "character" : "off" ,
        "reportOncePerFile": "off",
        "consistent-return" :"off",
        "no-underscore-dangle" :"off",
        "prettier/prettier": "error",
        "no-console" : "off"
    }
}
